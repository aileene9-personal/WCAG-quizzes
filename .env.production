# App Configuration
# Required: Application title and description
VITE_APP_TITLE="WCAG Quiz"
VITE_APP_DESCRIPTION="Test your WCAG knowledge"
VITE_APP_BASE_URL="/WCAG-quizzes/"
VITE_APP_ENV="production"

# API Configuration
# Required: Base URL for API endpoints
VITE_API_URL="https://aileene9-personal.github.io/WCAG-quizzes"

# Feature Flags
# Optional: Enable/disable features
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DARK_MODE=true

# Quiz Settings
# Required: Default quiz configuration
VITE_DEFAULT_QUESTIONS_COUNT=10
VITE_DEFAULT_TIME_PER_QUESTION=60

# Performance Settings
# Optional: Performance-related configurations
VITE_ENABLE_SOURCE_MAPS=false
VITE_ENABLE_COMPRESSION=true

# Security Settings
# Optional: Security-related configurations
VITE_ENABLE_HTTPS=true
VITE_ENABLE_CSP=true

# Cache Settings
# Optional: Cache-related configurations
VITE_CACHE_DURATION=3600
VITE_ENABLE_SERVICE_WORKER=true 